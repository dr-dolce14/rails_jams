<h3>Update Artist</h3>

<%= form_for @artist do |f| %>
  <%= f.label 'Artist Name' %><br>
  <%= f.text_field :name %><br>
 
  <%= f.label 'Artist Age' %><br>
  <%= f.text_area :age %><br>
  
  <%= f.label 'Artist Bio' %><br>
  <%= f.text_area :bio %><br>
 
  <%= f.submit "Submit Artist" %>
  <% end %>